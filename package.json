{
  "name": "validator",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf .turbo node_modules dist",
    "dev": "tsx ./src/index.ts",
    "test:firecrawl": "tsx ./src/test/firecrawl.pipeline.ts",
    "format": "prettier --check . --cache",
    "format-fix": "prettier --write . --cache",
    "lint": "eslint --cache --cache-location .cache/.eslintcache",
    "lint-fix": "eslint --cache --cache-location .cache/.eslintcache --fix",
    "start": "node ./dist/index.js",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@mendable/firecrawl-js": "^1.8.2",
    "@t3-oss/env-core": "^0.11.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "express": "^4.19.2",
    "node-fetch": "^3.3.2",
    "openai": "^6.8.0",
    "pg": "^8.16.3",
    "superjson": "^2.2.1",
    "tsafe": "^1.7.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.1",
    "@types/pg": "^8.15.6",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "dotenv-cli": "^7.4.2",
    "drizzle-kit": "^0.31.6",
    "eslint": "^9.15.0",
    "prettier": "^3.3.3",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
